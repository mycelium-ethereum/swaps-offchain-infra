{
  "name": "@mycelium-ethereum/swaps-keepers",
  "version": "0.1.3",
  "description": "A collection of keeper bots for Tracer's Total Return Swaps",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/tracer-protocol/TRS-Keepers.git",
  "author": "CalabashSquash",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "ts-node index.ts",
    "lint": "eslint . --ext .ts",
    "run:price-keeper": "ts-node ./scripts/runPriceKeeper.ts",
    "run:position-keeper": "ts-node ./scripts/runPositionKeeper.ts",
    "run:price-poller": "ts-node ./scripts/runPricePoller.ts",
    "docker:build-keepers": "rm -r ./dist && tsc && docker build -t swaps-keepers --platform linux/amd64 ."
  },
  "dependencies": {
    "@mycelium-ethereum/perpetual-swaps-contracts": "^1.0.3",
    "@mycelium-ethereum/swaps-js": "^1.0.0",
    "@types/node-fetch": "^2.6.1",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.1",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.2",
    "express": "^4.18.1",
    "nanoevents": "^7.0.1",
    "node-fetch": "2.6",
    "prom-client": "^14.0.1",
    "tiny-typed-emitter": "^2.1.0",
    "winston": "^3.8.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^29.3.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^29.3.1",
    "jest-websocket-mock": "^2.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
